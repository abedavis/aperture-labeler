onAnimationFrameCallback(){
        this._updateFocalPlane();
        super.onAnimationFrameCallback();
        this.sceneView.clear();
        this.sceneView.render();
        if(this.framesToRecord>0){
            this.sceneView.recordNextFrame()
            this.framesToRecord = this.framesToRecord-1;
            let dist = 10;
            let theta = (this.framesToRecord/this.totalFramesToRecord)*Math.PI*0.5;
            let campose = NodeTransform3D.LookAt(
                V3(dist*Math.cos(theta), 3, dist*Math.sin(theta)),
                V3(0.0,0.0,0.0),
                V3(0,-1,0)
            );

            console.log(campose.getMatrix().asPrettyString());

            this.camera.pose = campose;
        }
    }



